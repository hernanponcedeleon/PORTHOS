{flag1, flag2}

thread 0 {
while true {
  a <- 1;
  flag1.store(rx,a);
  f2 = flag2.load(rx);
  while (f2 >= 3) {
    f2 = flag2.load(rx)
  };
  a <- 3;
  flag1.store(rx,a);
  f2 = flag2.load(rx);
  if (f2 == 1) {
    a <- 2;
    flag1.store(rx,a);
    f2 = flag2.load(rx);
    while (f2 != 4) {
      f2 = flag2.load(rx)
    }
  };
  a <- 4;
  flag1.store(rx,a);
  f2 = flag2.load(rx);
  while (f2 >= 2) {
    f2 = flag2.load(rx)
  };
  f2 = flag2.load(rx);
  while ((2 <= f2) and (f2 <= 3)) {
    f2 = flag2.load(rx)
  };
  a <- 0;
  flag1.store(rx,a)
}
}

